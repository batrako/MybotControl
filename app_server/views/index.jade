extends layout

block content
 .container
      .row
        .col-md-4.col-sm-6.col-xs-12
          .panel.panel-primary
            .panel-heading Aldea Batrako
            .panel-body
              table
                tr
                  td
                    img(src='/images/Gold_Storage11.png',width='80', height='80')
                  td(style='width:80%')
                    .progress
                      .progress-bar.progress-bar-success(style='width: 80%') 8.000.000
                tr
                  td
                    img(src='/images/Elixir_Storage11.png',width='80', height='80')
                  td
                    .progress
                      .progress-bar.progress-bar-success(style='width: 100%') 10.000.000
                tr
                  td
                    img(src='/images/Dark_Elixir_Storage6.png',width='80', height='80')
                  td
                    .progress
                      .progress-bar.progress-bar-success(style='width: 30%') 3.000
                  tr
                    td
                      br
                      img(src='/images/builder.png',width='30', height='30')
                      label 2
                    td(align='right')
                      br
                      img(src='/images/gems_big_icon.png',width='30', height='30')
                    td
                      br
                      label 780
            .panel-foot 
